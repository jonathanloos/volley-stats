<%= bootstrap_form_with(model: team) do |form| %>
  <%= form.text_field :name %>

  <%= form.collection_select :head_coach_id, @team.users, :id, :full_name %>

  <%= form.collection_select :assistant_coach_id, @team.users, :id, :full_name %>

  <%= form.number_field :year %>

  <%= form.text_field :club %>

  <%= form.submit %>
<% end %>
